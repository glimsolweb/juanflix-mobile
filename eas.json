{
    "cli": {
        "version": ">= 16.2.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "android": {
                "buildType": "apk"
            }
        },
        "standalone": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview"
        },
        "production": {
            "autoIncrement": true,
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}
