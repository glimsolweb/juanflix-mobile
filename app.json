{
  "expo": {
    "name": "JuanFlixApp",
    "slug": "JuanFlixApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/juanflix-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/juanflix-icon.png"
      },
      "package": "com.juanflix.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/juanflix-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/Splash-Screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "https://mvn.jwplayer.com/content/repositories/releases/"
            ]
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "02123a9d-f7a2-49c5-8fc5-de8f3e22c78f"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/02123a9d-f7a2-49c5-8fc5-de8f3e22c78f"
    }
  }
}
