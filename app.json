{
    "expo": {
        "name": "juanflix-app",
        "slug": "juanflix-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/juanflix-icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/juanflix-icon.png"
            },
            "package": "com.juanflix.app"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/assets/images/juanflix-icon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./src/assets/images/Splash-Screen.png",
                    "resizeMode": "contain",
                    "backgroundColor": "#000000"
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "extraMavenRepos": [
                            "https://mvn.jwplayer.com/content/repositories/releases/"
                        ]
                    }
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "7302ed30-95e1-4aa3-bffe-c3f76880e0f6"
            }
        },
        "runtimeVersion": "1.0.0",
        "updates": {
            "url": "https://u.expo.dev/7302ed30-95e1-4aa3-bffe-c3f76880e0f6"
        }
    }
}
